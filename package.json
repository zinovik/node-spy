  "devDependencies": {
    "@types/jest": "29.5.11",
    "@types/node": "20.10.8",
    "@typescript-eslint/eslint-plugin": "6.18.1",
    "@typescript-eslint/parser": "6.18.1",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "jest": "29.7.0",
    "prettier": "3.1.1",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.2",
    "typemoq": "2.1.0",
